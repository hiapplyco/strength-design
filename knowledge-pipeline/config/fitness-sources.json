    ],
    "max_pages_per_term": 10,
    "language": "en",
    "content_filters": {
      "min_content_length": 500,
      "max_content_length": 10000,
      "exclude_categories": [
        "Deaths",
        "Controversies", 
        "Legal",
        "Political"
      ]
    }
  },
  "processing": {
    "min_content_length": 100,
    "max_content_length": 10000,
    "dedupe_threshold": 0.85,
    "quality_filters": {
      "min_quality_score": 0.6,
      "require_exercise_keywords": true,
      "filter_promotional": true,
      "filter_medical_advice": true
    },
    "content_enhancement": {
      "generate_summaries": true,
      "extract_key_points": true,
      "categorize_content": true,
      "extract_exercises": true,
      "difficulty_classification": true
    },
    "fitness_keywords": [
      "exercise",
      "workout", 
      "training",
      "fitness",
      "strength",
      "cardio",
      "muscle",
      "weight",
      "rep",
      "set",
      "routine",
      "form",
      "technique",
      "nutrition",
      "protein",
      "diet",
      "calories",
      "recovery",
      "flexibility",
      "endurance",
      "power",
      "speed",
      "agility",
      "balance",
      "coordination"
    ],
    "medical_disclaimer_keywords": [
      "consult doctor",
      "medical advice",
      "physician",
      "healthcare provider",
      "not medical advice",
      "seek professional help"
    ]
  },
  "output": {
    "format": "jsonl",
    "batch_size": 100,
    "include_metadata": true,
    "compression": true,
    "backup_copies": 3
  },
  "rate_limiting": {
    "reddit_requests_per_minute": 60,
    "wikipedia_requests_per_minute": 120,
    "concurrent_requests": 5,
    "retry_attempts": 3,
    "backoff_factor": 2
  },
  "data_validation": {
    "required_fields": [
      "title",
      "content", 
      "source",
      "content_type",
      "quality_score"
    ],
    "content_type_validation": {
      "exercise": {
        "required_keywords": ["exercise", "movement", "rep", "set"],
        "min_content_length": 50
      },
      "routine": {
        "required_keywords": ["routine", "workout", "program"],
        "min_content_length": 200
      },
      "nutrition": {
        "required_keywords": ["nutrition", "diet", "protein", "calories"],
        "min_content_length": 100
      },
      "guide": {
        "required_keywords": ["guide", "how to", "tutorial"],
        "min_content_length": 300
      }
    }
  },
  "storage": {
    "local_backup_path": "../data/backups/",
    "firebase_collection": "knowledge",
    "batch_upload_size": 25,
    "enable_compression": true,
    "retention_days": 365
  }
}