    ],
    "high_quality_flairs": [
      "Science",
      "Form Check", 
      "Program Review",
      "Training Tuesday",
      "Nutrition",
      "Equipment",
      "Physique Friday",
      "Daily Thread"
    ],
    "content_filters": {
      "exclude_keywords": [
        "promotion",
        "discount", 
        "buy now",
        "affiliate",
        "sponsor",
        "ad",
        "advertisement"
      ],
