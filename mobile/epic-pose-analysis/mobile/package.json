{
  "name": "mobile-working",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "detox test",
    "test:e2e:build": "detox build",
    "testlab:build:android": "eas build --platform android --profile testlab",
    "testlab:build:ios": "eas build --platform ios --profile testlab",
    "testlab:run:low": "gcloud firebase test android run --type instrumentation --app ./build/app-testlab.apk --test ./build/app-test.apk --device model=sailfish,version=27,locale=en_US,orientation=portrait --device model=j7xelte,version=24,locale=en_US,orientation=portrait",
    "testlab:run:mid": "gcloud firebase test android run --type instrumentation --app ./build/app-testlab.apk --test ./build/app-test.apk --device model=a10,version=29,locale=en_US,orientation=portrait --device model=oriole,version=31,locale=en_US,orientation=portrait",
    "testlab:run:high": "gcloud firebase test android run --type instrumentation --app ./build/app-testlab.apk --test ./build/app-test.apk --device model=blueline,version=28,locale=en_US,orientation=portrait --device model=redfin,version=30,locale=en_US,orientation=portrait --device model=panther,version=33,locale=en_US,orientation=portrait",
    "testlab:run:all": "gcloud firebase test android run --type instrumentation --app ./build/app-testlab.apk --test ./build/app-test.apk --config .testlabrc.yml",
    "testlab:run:ios": "gcloud firebase test ios run --test ./build/ios-test.zip --config .testlabrc.yml",
    "testlab:analyze": "node scripts/analyze-testlab-results.js",
    "test:performance": "jest --testPathPattern=performance --verbose",
    "test:network": "jest --testMatch='**/__tests__/performance/networkConditionTests.test.js' --verbose",
    "test:security": "jest --testPathPattern=security --verbose",
    "benchmark": "node scripts/performance-benchmark.js",
    "benchmark:compare": "node scripts/performance-benchmark.js compare",
    "uat:analyze": "node scripts/analyze-uat-results.js"
  },
  "dependencies": {
    "@expo/metro-config": "0.20.17",
    "@expo/metro-runtime": "^5.0.4",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-navigation/bottom-tabs": "^7.4.6",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.7",
    "expo": "~53.0.20",
    "expo-av": "~15.1.7",
    "expo-blur": "~14.1.5",
    "expo-calendar": "~14.1.4",
    "expo-camera": "~16.1.11",
    "expo-clipboard": "^7.1.5",
    "expo-dev-client": "~5.2.4",
    "expo-device": "^7.1.4",
    "expo-file-system": "~18.1.11",
    "expo-haptics": "~14.1.4",
    "expo-image-picker": "^16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-media-library": "~17.1.7",
    "expo-sharing": "~13.1.5",
    "expo-status-bar": "~2.2.3",
    "firebase": "^12.1.0",
    "lucide-react-native": "^0.539.0",
    "metro": "^0.82.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.12.1",
    "react-native-view-shot": "^4.0.3",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/react-native": "^12.4.3",
    "@testing-library/jest-native": "^5.4.3",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.4",
    "react-test-renderer": "19.0.0",
    "detox": "^20.18.3",
    "@babel/preset-typescript": "^7.23.3"
  },
  "private": true
}
